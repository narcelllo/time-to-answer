  <h1>Admins</h1>
  <p>Find me in app/views/admins_backoffice/admins/index.html.erb</p>

  <div>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th scope="col">Id</th>
          <th scope="col">Email</th>
          <th scope="col">created at</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        <% @admins.each do |admin|%>
        <tr>
          <td><%= admin.id %></td>
          <td><%= admin.email %></td>
          <td><%= admin.created_at.strftime("%d/%m/%Y %H:%M") %></td>
          <td style=" width: 150px" >
              <%= link_to 'Edit', edit_admins_backoffice_admin_path(admin), class: "btn btn-info" %>
            <button type="button" class="btn btn-danger">Delet</button>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
